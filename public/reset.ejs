<div class="header">
	<div class="welcome">
		Welcome
	</div>
	<div class="brand">
		<a href="#/main"><img src="/images/logo.png" width="300px"></a>
	</div>
</div>
<div class="formSecond">
	<form class="form-auth" ng-submit="resetPassword()">
		<label>Enter new password</label>
		<input type="text" ng-model="user.password" placeholder="new password" required></input>
		<label>Verification Code: </label>
		<div id="captcha"></div>
		<input type="text" placeholder="enter verification code" id="captchaText" required></input>
		<button type="submit" value="forgot"><div class="text">Next</div></button>
		<p class="text-warning"><br>{{error_message}}</p>
	</form>
</div>
